<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="1" parallel="tests" thread-count="3">
	<test thread-count="5" name="Chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.yatra.yatrapagetest.IndexPageTest" />
			<class name="com.yatra.yatrapagetest.Searchflightpagetest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.yatra.yatrapagetest.IndexPageTest" />
			<class name="com.yatra.yatrapagetest.Searchflightpagetest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.yatra.yatrapagetest.IndexPageTest" />
			<class name="com.yatra.yatrapagetest.Searchflightpagetest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
